{
  "manifest_version": 2,
  "name": "Facebook Covert",
  "version": "1.0.0",
  "description": "See full content of unread messages on Facebook.com",
  "content_scripts": [{
    "css": ["css/main.css"],
    "js": [],
    "matches": ["https://www.facebook.com/*"]
  }],
  "icons": {
    "16": "icons/facebook-covert-16x16.png",
    "32": "icons/facebook-covert-32x32.png",
    "192": "icons/facebook-covert-192x192.png",
    "512": "icons/facebook-covert-512x512.png",
    "800": "icons/facebook-covert.png"
  }
}
