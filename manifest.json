{
  "manifest_version": 2,
  "name": "Facebook Covert",
  "version": "1.0.0",
  "description": "See full content of unread messages on Facebook.com",
  "author": "Antoine Demailly <antoine.demailly1@gmail.com> (https://www.antoinedemailly.fr)",
  "content_scripts": [{
    "css": ["css/main.css"],
    "js": [],
    "matches": ["https://www.facebook.com/*"]
  }],
  "icons": {
    "16": "icons/facebook-covert@16.png",
    "32": "icons/facebook-covert@32.png",
    "48": "icons/facebook-covert@48.png",
    "128": "icons/facebook-covert@128.png",
    "800": "icons/facebook-covert@800.png"
  }
}
